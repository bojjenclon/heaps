{
  "defaultSeverity": "INFO",
  "extendsConfigPath": "",
  "numberOfCheckerThreads": 5,
  "baseDefines": [],
  "defineCombinations": [],
  "checks": [
    {
      "type": "Anonymous",
      "props": {}
    },
    {
      "type": "AvoidStarImport",
      "props": {}
    },
    {
      "type": "BlockBreakingConditional",
      "props": {}
    },
    {
      "type": "CodeSimilarity",
      "props": {
        "severityIdentical": "WARNING",
        "thresholdIdentical": 60,
        "thresholdSimilar": 120
      }
    },
    {
      "type": "Dynamic",
      "props": {}
    },
    {
      "type": "ExtendedEmptyLines",
      "props": {
        "max": 1,
        "skipSingleLineTypes": true,
        "defaultPolicy": "upto",
        "ignore": [],
        "none": [
          "beforePackage",
          "betweenImports",
          "beforeUsing",
          "typeDefinition",
          "afterLeftCurly",
          "beforeRightCurly"
        ],
        "exact": [],
        "upto": [],
        "atleast": []
      }
    },
    {
      "type": "FieldDocComment",
      "props": {
        "tokens": [
          "ABSTRACT_DEF",
          "CLASS_DEF",
          "ENUM_DEF",
          "INTERFACE_DEF",
          "TYPEDEF_DEF"
        ],
        "fieldType": "VARS",
        "modifier": "PUBLIC",
        "requireParams": true,
        "requireReturn": true,
        "excludeNames": [
          "new",
          "toString"
        ],
        "ignoreOverride": true
      }
    },
    {
      "type": "Final",
      "props": {}
    },
    {
      "type": "Indentation",
      "props": {
        "character": "  ",
        "ignoreConditionals": false,
        "ignoreComments": false,
        "wrapPolicy": "larger",
        "conditionalPolicy": "ignore"
      }
    },
    {
      "type": "IndentationCharacter",
      "props": {
        "character": "space",
        "ignorePattern": "^$"
      }
    },
    {
      "type": "LeftCurly",
      "props": {
        "tokens": [
          "CLASS_DEF",
          "ENUM_DEF",
          "ABSTRACT_DEF",
          "TYPEDEF_DEF",
          "INTERFACE_DEF",
          "OBJECT_DECL",
          "FUNCTION",
          "FOR",
          "IF",
          "WHILE",
          "SWITCH",
          "TRY",
          "CATCH",
          "REIFICATION",
          "ARRAY_COMPREHENSION"
        ],
        "option": "nl",
        "ignoreEmptySingleline": false,
        "ignoreSingleline": false
      }
    },
    {
      "type": "NeedBraces",
      "props": {
        "allowSingleLineStatement": false,
        "tokens": [
          "FOR",
          "IF",
          "ELSE_IF",
          "WHILE",
          "DO_WHILE"
        ],
        "severity": "WARNING"
      }
    },
    {
      "type": "OperatorWhitespace",
      "props": {
        "assignOpPolicy": "around",
        "unaryOpPolicy": "ignore",
        "ternaryOpPolicy": "ignore",
        "arithmeticOpPolicy": "ignore",
        "compareOpPolicy": "around",
        "bitwiseOpPolicy": "ignore",
        "boolOpPolicy": "ignore",
        "intervalOpPolicy": "ignore",
        "arrowPolicy": "ignore",
        "arrowFunctionPolicy": "ignore",
        "oldFunctionTypePolicy": "ignore",
        "newFunctionTypePolicy": "ignore"
      }
    },
    {
      "type": "RedundantAccessMeta",
      "props": {
        "prohibitMeta": false
      }
    },
    {
      "type": "RedundantAllowMeta",
      "props": {
        "prohibitMeta": false
      }
    },
    {
      "type": "RedundantModifier",
      "props": {
        "enforcePublicPrivate": false,
        "enforcePublic": false,
        "enforcePrivate": false
      }
    },
    {
      "type": "Return",
      "props": {
        "allowEmptyReturn": true,
        "enforceReturnType": false,
        "enforceReturnTypeForAnonymous": false
      }
    },
    {
      "type": "SeparatorWhitespace",
      "props": {
        "dotPolicy": "none",
        "commaPolicy": "after",
        "semicolonPolicy": "after",
        "allowTrailingComma": false
      }
    },
    {
      "type": "Spacing",
      "props": {
        "spaceIfCondition": "should",
        "spaceForLoop": "any",
        "spaceWhileLoop": "any",
        "spaceSwitchCase": "any",
        "spaceCatch": "any",
        "spaceAroundBinop": false,
        "noSpaceAroundUnop": false,
        "ignoreRangeOperator": false
      }
    },
    {
      "type": "StringLiteral",
      "props": {
        "policy": "doubleAndInterpolation",
        "allowException": true
      }
    },
    {
      "type": "TODOComment",
      "props": {
        "format": "^\\s*(TODO|FIXME|HACK|XXX|BUG)"
      }
    },
    {
      "type": "Trace",
      "props": {}
    },
    {
      "type": "TrailingWhitespace",
      "props": {}
    },
    {
      "type": "Type",
      "props": {
        "ignoreEnumAbstractValues": true
      }
    },
    {
      "type": "TypeDocComment",
      "props": {
        "tokens": [
          "ABSTRACT_DEF",
          "CLASS_DEF",
          "ENUM_DEF",
          "INTERFACE_DEF",
          "TYPEDEF_DEF"
        ]
      }
    },
    {
      "type": "UnnecessaryConstructor",
      "props": {}
    },
    {
      "type": "UnusedImport",
      "props": {
        "ignoreModules": [],
        "moduleTypeMap": {}
      }
    },
    {
      "type": "UnusedLocalVar",
      "props": {}
    }
  ],
  "exclude": {},
  "version": 1
}